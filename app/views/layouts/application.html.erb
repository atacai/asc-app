<!DOCTYPE html>
<html>
  <head>
    <title>Hotels App</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'https://cdn.datatables.net/v/bs4/dt-1.10.18/datatables.min.css' %>
    <%= stylesheet_link_tag    'https://use.fontawesome.com/releases/v5.6.1/css/all.css', integrity:'sha384-gfdkjb5BdAXd+lj+gudLWI+BXq4IuLW5IT+brZEZsLFm++aCMlF1V92rMkPaX4PP', crossorigin: "anonymous" %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'https://cdn.datatables.net/v/bs4/dt-1.10.18/datatables.min.js' %>
    <%= javascript_pack_tag    'application' %>

  </head>

  <body>
    <%= yield %>
  </body>
</html>
